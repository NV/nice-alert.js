{
  "manifest_version": 2,
  "name": "Nice alert",
  "version": "1.3.1",
  "description": "Makes alert box suck less",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "64": "icon_64.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["nice_alert.js"],
  "update_url": "http://clients2.google.com/service/update2/crx"
}
